{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/reset-password",
            "destination": "/public/reset-password.html"
        },
        {
            "source": "/admin",
            "destination": "/public/admin.html"
        },
        {
            "source": "/:path*",
            "destination": "/public/:path*"
        }
    ]
}